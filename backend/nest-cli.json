{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "tsconfig.json",
    "assets": [
      {
        "include": "**/*.hbs",
        "outDir": "dist"
      },
      {
        "include": "**/*.html",
        "outDir": "dist"
      },
      {
        "include": "**/*.json",
        "outDir": "dist"
      }
    ],
    "watchAssets": true
  },
  "generateOptions": {
    "spec": true,
    "flat": false
  },
  "monorepo": false,
  "root": ".",
  "projects": {}
}
